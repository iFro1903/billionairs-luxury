  "description": "Exklusive Milliardärs-Website mit luxuriöser Uhr - 500.000 CHF Premium Experience",
  "main": "index.html",
  "engines": {
    "node": ">=18.x"
  },
  "scripts": {
    "start": "node stripe-server.js",
    "dev": "nodemon stripe-server.js",
    "start:production": "set NODE_ENV=production && node stripe-server.js",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:ui": "playwright test --ui",
    "test:report": "playwright show-report",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "build": "tsc && echo Production build ready",
    "pm2:start": "pm2 start stripe-server.js --name billionairs",
    "pm2:stop": "pm2 stop billionairs",
    "pm2:restart": "pm2 restart billionairs",
    "pm2:logs": "pm2 logs billionairs"
  },
  "keywords": [
    "luxury",
    "billionaire",
    "exclusive",
    "timepiece",
    "premium",
    "switzerland"
  ],
  "author": "Luxury Digital Experiences",
  "license": "Private",
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/node": "^24.9.1",
    "live-server": "^1.2.2",
    "nodemon": "^3.1.10",
    "terser": "^5.44.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.10.3",
    "@upstash/redis": "^1.35.6",
    "@vercel/postgres": "^0.10.0",
    "bcryptjs": "^2.4.3",
